{
  "compilerOptions": {
    "baseUrl": "./src",
    "paths": {
      "@components/*": [
        "components/*"
      ],
      "@screens/*": [
        "screens/*"
      ],
      "@navigation/*": [
        "navigation/*"
      ],
      "@services/*": [
        "services/*"
      ],
      "@utils/*": [
        "utils/*"
      ],
      "@types/*": [
        "types/*"
      ],
      "@assets/*": [
        "assets/*"
      ],
      "@hooks/*": [
        "hooks/*"
      ]
    },
    "allowSyntheticDefaultImports": true,
    "jsx": "react"
  },
  "extends": "expo/tsconfig.base"
}
